{
	"name": "saaqi-portfolio-website",
	"version": "2.1.0",
	"description": "Saqib Islam's portfolio website, a showcase of my work, projects, and achievements. Explore my expertise and passion in web development and design. Join me on this creative journey. #portfolio #webdevelopment #design",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build --emptyOutDir && bun ./src/sitemap.js",
		"build-bootstrap": "sass --no-source-map ./src/styles/bs-dynamic.scss ./src/styles/bootstrap.css",
		"check": "svelte-check",
		"dev": "vite dev",
		"deploy": "git checkout main && git merge sveltekit && git push && git checkout sveltekit",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.28.0",
		"@fullhuman/postcss-purgecss": "^7.0.2",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.21.3",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"autoprefixer": "^10.4.21",
		"cssnano": "^7.0.7",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.1",
		"globals": "^16.2.0",
		"postcss": "^8.5.4",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"sass-embedded": "^1.89.1",
		"svelte": "^5.33.18",
		"svelte-check": "^4.2.1",
		"terser": "^5.41.0",
		"vite": "^6.3.5"
	},
	"dependencies": {
		"@fontsource-variable/montserrat": "^5.2.6",
		"@sveltejs/enhanced-img": "^0.6.0",
		"@types/bootstrap": "^5.2.10",
		"bootstrap": "^5.3.6",
		"install": "^0.13.0"
	}
}
