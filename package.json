{
	"name": "saaqi-portfolio-website",
	"version": "2.1.4",
	"description": "Saqib Islam's portfolio website, a showcase of my work, projects, and achievements. Explore my expertise and passion in web development and design. Join me on this creative journey. #portfolio #webdevelopment #design",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "bun ./src/sitemap.ts && vite build --emptyOutDir",
		"build-bootstrap": "sass --no-source-map ./src/styles/bs-dynamic.scss ./src/styles/bootstrap.css",
		"check": "svelte-check",
		"dev": "vite dev",
		"deploy": "git checkout main && git pull && git merge sveltekit && git push && git checkout sveltekit",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.29.0",
		"@fullhuman/postcss-purgecss": "^7.0.2",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.2",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"autoprefixer": "^10.4.21",
		"cssnano": "^7.0.7",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.10.0",
		"globals": "^16.2.0",
		"postcss": "^8.5.6",
		"prettier": "^3.6.1",
		"prettier-plugin-svelte": "^3.4.0",
		"sass-embedded": "^1.89.2",
		"svelte": "^5.34.8",
		"svelte-check": "^4.2.2",
		"terser": "^5.43.1",
		"typescript-eslint": "^8.35.1",
		"vite": "^6.3.5"
	},
	"dependencies": {
		"@fontsource-variable/inter": "^5.2.6",
		"@fontsource-variable/space-grotesk": "^5.2.8",
		"@sveltejs/enhanced-img": "^0.6.1",
		"@types/bootstrap": "^5.2.10",
		"@types/node": "^24.0.10",
		"bootstrap": "^5.3.7",
		"install": "^0.13.0"
	}
}
